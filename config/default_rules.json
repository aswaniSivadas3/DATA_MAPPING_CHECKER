        "AAE",
        "ADM",
        "ASE",
        "BVR",
        "CB",
        "CDO",
        "CHQ",
        "CIG",
        "COD",
        "EFF",
        "ESP",
        "LCA",
        "LCD",
        "LCM",
        "LCN",
        "MAD",
        "NDE",
        "PAG",
        "PRE",
        "RBA",
        "RBO",
        "RDO",
        "SDD",
        "VCO",
        "VDR",
        "VIR"
      ]
    },
  
  "EligibleForDefaultInterests": {
      "type": "number",
      "length": 1,
      "mandatory": false,
      "nature": "N",
      "notes": "Empty or 1 => yes 0 => No Interest indicator field"
    },
  "OrderStatusDate": {
      "type": "string",
      "length": 10,
      "mandatory": true,
      "nature": "AN",
      "notes": "Format YYYYMMDD"
    }
  
  
}
